<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.hackathon.cubicle">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Cubicle">
        <activity
            android:name="com.hackathon.cubicle.employee.ui.view.activity.EmployeeActivity" />

        <activity android:name=".general.LoginActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".general.SignUpActivity"/>

        <activity android:name=".admin.ui.view.activity.AdminActivity"/>
        <activity android:name=".admin.ui.view.activity.AddEmployeeDialog"/>
    </application>

</manifest>